{
    "name": "nemozar/data-transfer-object",
    "description": "Data transfer objects with batteries included",
    "keywords": [
        "nemozar",
        "data-transfer-object"
    ],
    "homepage": "https://github.com/nemozar/data-transfer-object",
    "license": "MIT",
    "authors": [
        {
            "name": "",
            "email": "",
            "homepage": "",
            "role": ""
        }
    ],
    "require": {
        "php": "^7.2|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "suggest": {
        "phpstan/phpstan": "Take advantage of checkUninitializedProperties with \\Nemozar\\DataTransferObject\\PHPstan\\PropertiesAreAlwaysInitializedExtension"
    },
    "autoload": {
        "psr-4": {
            "Nemozar\\DataTransferObject\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nemozar\\DataTransferObject\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    }
}
